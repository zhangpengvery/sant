/* pages/address/address.wxss */
.content{
  width: 94%;
  height: auto;
  margin: 0 3%;
  overflow: hidden;
}
.con-list{
  width: 100%;
  display: flex;
  justify-content: space-between;
  height: 136rpx;
  border-bottom: 2rpx solid #F4F4F4;
  margin-top: 20rpx;
  align-items: center;
}
.con-list .con-left{
  width: 40rpx;
  height: 40rpx;
  border-radius: 50%;
  background: #F9B236;
  text-align: center;
  color: #fff;
  font-size: 24rpx;
  line-height: 40rpx;
}
.dizhi{
  font-size: 26rpx;
  color: #333;
  margin: 0 20rpx;
}
.dizhi .user{
  display: flex;
  color: #A1A1A1;
  font-size: 24rpx;
  margin-top: 20rpx;
}
.user .user-name{
  margin-right: 20rpx;
}
.xiugai{
  color: #989898;
}
.ljzf{
  position: fixed;
  bottom: 40rpx;
  left: 0;
  width: 94%;
  margin: 0 3%;
  height: 94rpx;
  border-radius: 94rpx;
  background: #F9B236;
  color: #fff;
  text-align: center;
  line-height: 94rpx;
  font-size: 32rpx;
  font-weight: bold;
  z-index: 10;
}
.wushujucs{
  width: 100%;
  margin-top: 300rpx;
  text-align: center;
  font-size: 24rpx;
  color: #ababab;
}
.wushu-img{
  width: 44%;
  margin: 0 28%;
  height: 180rpx;
}
.wushu-img image{
  width: 100%;
  height: 100%;
}