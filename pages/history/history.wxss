/* pages/history/history.wxss */
@import "/assets/fonts/iconfont.wxss";
.mapbox{
  position: relative;
  overflow: hidden;
}
.customCallout{
  width: 100rpx;
  height: 100rpx;
  text-align: center;
}
/* .icon{
  width: 50rpx;
  height: 50rpx;
  border-radius: 50%;
} */
.icon{
  height: 40rpx;
}
.gittime{
  position: fixed;
  top: 100rpx;
  left: 30rpx;
  width: 220rpx;
  height: 60rpx;
  box-shadow: 0px 0px 3px rgba(0, 0, 0, 0.16);
  border-radius: 32rpx;
  background: #fff;
  font-size: 24rpx;
  color: #777;
}
.gittime-box{
  height: 60rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.picker{
  margin-left:8rpx;
}
.nulllist{
  width: 90%;
  height: auto;
  margin: 0 5%;
  display: flex;
  justify-content: end;
  flex-wrap: wrap;
}
.nulllist-box{
  width: 20%;
  display: flex;
  flex-direction: column;
  align-items: center;
  overflow: hidden;
  margin-top: 20rpx;
  text-align: center;
}
.nulllist-tx{
  width: 80rpx;
  height: 80rpx;
  border-radius: 50%;
  border: 4rpx solid #fff;
}
.nulllist-tx2{
  border: 4rpx solid #FA8905;
}
.nulllist-tx image{
  width: 100%;
  height: 100%;
  border-radius: 50%;
}
.nulllist-name{
  font-size: 28rpx;
}
.cishu{
  width: 370rpx;
  height: 60rpx;
  border-radius: 32rpx;
  box-shadow: 0px 0px 3px rgba(0, 0, 0, 0.16);
  position: absolute;
  bottom: 20rpx;
  right: 30rpx;
  background: #fff;
  display: flex;
  justify-content: center;
  font-size: 28rpx;
  text-align: center;
  line-height: 60rpx;
}
.cishu-right{
  width: 90rpx;
  height: 60rpx;
  overflow: hidden;
}
.cishu .cishu-text{
  color: #F9B236;
}
.cishu .cishu-left{
  margin: 0 10rpx;
}
.kong{
  width: 100%;
  text-align: center;
  height: 100rpx;
  line-height: 100rpx;
  color: #777;
  font-size: 26rpx;
}