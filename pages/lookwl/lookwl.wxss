/* pages/lookwl/lookwl.wxss */
page{
  background: #f6f6f6;
}
.home-navimg{
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 460rpx;
  z-index: -1;
}
.home-navimg image{
  width: 100%;
  height: 100%;
}
.textyun{
  font-size: 40rpx;
  font-weight: 500;
  text-align: center;
}
.car{
  width: 104rpx;
  height: 70rpx;
  margin: 30rpx auto 10rpx auto;
}
.car image{
  width: 100%;
  height: 100%;
}
.address{
  display: flex;
  width: 80%;
  margin: 0 10%;
  justify-content: space-between;
  align-items: center;
}
.add-text{
  font-size: 26rpx;
  height: 50rpx;
  line-height: 66rpx;
}
.footer{
  width: 84%;
  margin: 30rpx 5% 0 5%;
  border-radius: 20rpx;
  background: #fff;
  padding:0 3%;
}
.title{
  height: 100rpx;
  line-height: 100rpx;
  font-size: 34rpx;
}
.footer-box{
  position: relative;
  display: flex;
  justify-content: end;
  height: 60rpx;
  font-size: 28rpx;
}
.footer-box .iconfont{
  position: absolute;
  top: 0;
  right: 30rpx;
  color: #777;
}
.wuliu{
  width: 84%;
  margin: 30rpx 5% 0 5%;
  border-radius: 20rpx;
  background: #fff;
  padding:30rpx 3%;
}
.wuliu-box{
  display: flex;
  justify-content: flex-start;
  align-items: center;
}
.wuliu-left{
  position: relative;
  height: 100rpx;
}
.wuliu-right{
  margin-left: 50rpx;
}
.qiuqiu{
  position: absolute;
  top: 10rpx;
  left: 10rpx;
  width: 14rpx;
  height: 14rpx;
  border-radius: 50%;
  background: #CDCDCD;
  z-index: 10;
}
.xianxian{
  position: absolute;
  top: 10rpx;
  left: 16rpx;
  width: 0;
  height: 100rpx;
  border-left: 2rpx solid #E6E6E6;
}
.wuliu-time{
  color: #777;
  font-size: 26rpx;
}
.wuliu-add{
  font-size: 24rpx;
  color: #333;
}