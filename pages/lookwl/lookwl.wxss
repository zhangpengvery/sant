/* pages/lookwl/lookwl.wxss */
@import "/assets/fonts/iconfont.wxss";
page{
  background: #f6f6f6;
}
.home-navimg{
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 460rpx;
  z-index: -1;
}
.home-navimg image{
  width: 100%;
  height: 100%;
}
.textyun{
  font-size: 40rpx;
  font-weight: 500;
  text-align: center;
}
.car{
  width: 104rpx;
  height: 70rpx;
  margin: 30rpx auto 10rpx auto;
}
.car image{
  width: 100%;
  height: 100%;
}
.address{
  display: flex;
  width: 80%;
  margin: 0 10%;
  justify-content: space-between;
  align-items: center;
}
.add-text{
  font-size: 26rpx;
  height: 50rpx;
  line-height: 66rpx;
}
.footer{
  width: 85%;
  margin: 30rpx 5% 0 5%;
  border-radius: 20rpx;
  background: #fff;
  padding-left: 5%
}
.title{
  height: 100rpx;
  line-height: 100rpx;
  font-size: 34rpx;
}
.footer-box{
  display: flex;
  justify-content: end;
  height: 60rpx;
  font-size: 28rpx;
}