/* pages/home/home.wxss */
@import "/assets/fonts/iconfont.wxss";
.home-navimg{
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 470rpx;
  z-index: -1;
  background: linear-gradient(180deg, #F9B236 0%, #F9B236 62%, rgba(249, 178, 54, 0) 100%);
}
.header{
  position: relative;
  color: #fff;
  font-size: 30rpx;
  margin-left: 3%;
  z-index: 1;
}
.header image{
  width: 260rpx;
  height: 60rpx;
}
.home-swiper{
  height: 400rpx;
  width: 94%;
  margin: 0 3%;
}
.home-swiper swiper-item .home-conent{
  height: 340rpx;
  width: 100%;
  border-radius: 15px;
}
.home-swiper swiper-item .home-conent image{
  width: 100%;
  height: 100%;
  border-radius: 15px;
}
.icon-fater{
  position: relative;
  width: 94%;
  height: 336rpx;
  display: flex;
  justify-content: space-around;
  flex-wrap: wrap;
  text-align: center;
  margin: 0 3%;
  margin-top: 20rpx;
  background: #FFFFFF;
  box-shadow: 0px 3px 6px rgba(0, 0, 0, 0.16);
  border-radius: 14px;
  z-index: 1;
}
.icon-fater .icon-conent{
  display: flex;
  justify-content: center;
  flex-direction: column;
  width: 25%;
  height: 168rpx;
}
.icon-fater .icon-conent text{
  color: #343232;
  font-size: 24rpx;
  font-weight: 400;
}
.icon-fater .icon-conent .icon-img{
  width: 80rpx;
  height: 80rpx;
  line-height: 102rpx;
  margin-left: 49rpx;
  margin-bottom: 15rpx;
  box-shadow: 0px 3px 6px rgba(0, 0, 0, 0.1);
  border-radius: 50%;
}
.icon-fater .icon-conent .icon-img image{
  width: 48rpx;
  height: 44rpx;
}
.home-options{
  width: 94%;
  margin: 0 3%;
  height: 100rpx;
  display: flex;
  position: relative;
  line-height: 100rpx;
}
.home-options .zuo{
  position: absolute;
  top: 50%;
  width: 8rpx;
  height: 36rpx;
  margin-top: -18rpx;
  background-color: #F9B236;
}
.home-options text{
  display: block;
  margin-left: 20rpx;
  color: #343232;
  font-size: 17px;
  font-weight: bold;
}
.bp-fater{
  display: flex;
  width: 94%;
  height:198rpx;
  margin: 0 3%;
  justify-content: space-between;
}
.bp-fater .bp-img{
  position: relative;
  width: 48%;
  color: #fff;
}
.bp-fater .img-title{
  position: absolute;
  top: 30rpx;
  left: 20rpx;
  font-size: 30rpx;
  font-weight: 600;
}
.bp-fater .img-text{
  position: absolute;
  width: 130rpx;
  top: 80rpx;
  left: 20rpx;
  font-size: 20rpx;
  line-height: 30rpx;
}
.bp-fater .bp-img image{
  width: 100%;
  height: 100%;
  border-radius: 20rpx;
}
.hhl-fater{
  display: flex;
  width: 94%;
  margin: 0 3%;
  justify-content: space-between;
}
.hhl-box{
  position: relative;
  width: 48%;
  height: 300rpx;
}
.hhl-cz{
  position: absolute;
  top: 6rpx;
  left: 12rpx;
  color: #fff;
  font-size: 20rpx;
}
.hhl-text{
  position: absolute;
  top: 60rpx;
  left: 20rpx;
  color: #CB5B4C;
}
.hhl-name{
  font-size: 30rpx;
  font-weight: bold;
}
.hhl-con{
  margin: 5rpx 0 15rpx 0;
  font-size: 20rpx;
}
.hhl-qkk{
  width: 70rpx;
  height: 30rpx;
  font-size: 18rpx;
  background: #CB5B4C;
  color: #fff;
  border-radius: 8rpx;
  line-height: 30rpx;
  text-align: center;
}
.hhl-footer{
  position: absolute;
  bottom: 4rpx;
  left: 30rpx;
  color: #fff;
}
.hhl-img{
  position: absolute;
  width: 240rpx;
  height: 136rpx;
  top: 110rpx;
  right: -14rpx;
}
.hhl-img image{
  width: 100%;
  height: 100%;
}
.hhl-ftext{
  position: absolute;
  font-weight: bold;
  bottom: 10rpx;
  right: 30rpx;
  font-size: 28rpx;
  -webkit-background-clip:text;
  color: transparent;
  background-image: linear-gradient(359deg, #F3D686 0%, #FFF9E5 100%);
}
.hhl-box image{
  width: 100%;
  height: 100%;
}
.options-fater{
  width: 94%;
  margin: 50rpx 3% 0 3%;
  height: 445rpx;
  text-align: center;
  background: #FFBA43;
  border-radius: 20rpx;
}
.options-header{
  display: flex;
  justify-content: space-between;
  width: 96%;
  height: 80rpx;
  margin: 0 2%;
  line-height: 80rpx;
}
.options-left{
  display: flex;
  height: 80rpx;
  align-items: center;
}
.options-left image{
  width: 28rpx;
  height: 28rpx;
}
.options-left .options-rmpj{
  font-size: 34rpx;
  font-weight: bold;
  margin-left: 10rpx;
}
.options-left .options-gd{
  font-size: 24rpx;
  color: #333;
  margin-right: 10rpx;
}
.options-new{
  width: 96%;
  margin: 0 2%;
  height: 350rpx;
  background: #fff;
  border-radius: 8rpx 8rpx 16rpx 16rpx;
  display: flex;
  justify-content: space-between;
}
 .options-child{
  position: relative;
  width: 33%;
  height: 350rpx;
  display: flex;
  flex-direction: column;
}
.img-box{
  width: 80%;
  height: 200rpx;
  padding: 10%;
}
.img-box image{
  width: 100%;
  height: 100%;
}
.child-footer{
  width: 100%;
  height: 92rpx;
}
 .options-price{
  margin-top: 10rpx;
  color: #D90F0F;
  font-size: 26rpx;
}
.options-name{
  width: 70%;
  margin: 0 15%;
  font-size: 26rpx;
  height: 45rpx;
  line-height: 45rpx;
  background: #FFBA43;
  border-radius: 4rpx;
}
.options-child .options-price::before {
  content: "￥";
  color: #D90F0F;
}
 .options-child .options-rm{
  position: absolute;
  top: 0;
  left: 0;
  width: 72rpx;
  height: 32rpx;
  background: linear-gradient(90deg, #F12D2D 0%, #FFB639 100%);
  border-radius: 0 0px 20rpx 0px;
  color: #fff;
  font-size: 19rpx;
  line-height: 32rpx;
  text-align: center;
}
.urgent{
  position: fixed;
  display: flex;
  right: 0;
  bottom: 200rpx;
  height: 90rpx;
  width: 120rpx;
  border-radius: 50rpx 0 0 50rpx;
  background: #F9B236;
  color: #fff;
  text-align: center;
  align-items: center;
}
.urgent .iconfont{
  width: 60%;
  margin-left: 10%;
  font-size: 40rpx;
}
.urgent-text{
  font-size: 20rpx;
}
.service{
  position: fixed;
  display: flex;
  right: 0;
  bottom: 100rpx;
  height: 90rpx;
  line-height: 90rpx;
  width: 120rpx;
  border-radius: 50rpx 0 0 50rpx;
  background: #F9B236;
  color: #fff;
  text-align: center;
}
.service .iconfont{
  width: 50%;
  margin-left: 10%;
  font-size: 40rpx;
}
@import "/assets/fonts/iconfont.wxss";
.list{
  width: 94%;
  height: auto;
  margin: 0 3%;
  display: flex;
  justify-content: space-between;
  flex-wrap: wrap;
  text-align: center;
}
.list .list-content{
  position: relative;
  width: 44%;
  height: 295rpx;
  padding: 2%;
  border-radius: 20rpx;
  box-shadow: 0px 0px 10px rgba(0, 0, 0, 0.07);
  margin-bottom: 20rpx;
}
.list-img{
  position: absolute;
  top: 0;
  left: 0;
  width: 70rpx;
  height: 70rpx;
}
.list-img .img1{
  width: 100%;
  height: 100%;
}
.list-content .img-con{
  width: 284rpx;
  height: 175rpx;
  border-radius: 10rpx 10rpx 0 0;
  margin-top: 10rpx;
}
.list .list-content .list-name{
  text-align: left;
  padding-left: 3%;
  width: 93%;
  font-size: 28rpx;
  color: #343232;
  height: 40rpx;
  display: block;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  margin: 10rpx 0 5rpx 0;
}
.list .list-content .list-price{
  display: block;
  padding-left: 3%;
  width: 93%;
  text-align: left;
  font-size: 28rpx;
  color: #D90F0F;
}
.list .list-content .list-price ::before{
  content: "￥";
}
.list .list-content .list-xin{
  position: absolute;
  font-size: 24rpx;
  bottom: 30rpx;
  right: 30rpx;
  color: #F9B236;
}
.dixian{
  width: 100%;
  height: 50rpx;
  text-align: center;
  font-size: 24rpx;
  color: #ababab;
}