/* pages/evenlist/evenlist.wxss */
page{
  background: #F6F6F6;
}
.list{
  width: 90%;
  margin: 0 5%;
  height: auto;
  overflow: hidden;
}
.box{
  width: 100%;
  height: 300rpx;
  border-radius: 20rpx;
  background: #fff;
  margin-top: 20rpx;
}
.box image{
  width: 100%;
  height: 230rpx;
  border-radius: 20rpx 20rpx 0  0;
}
.box-footer{
  width: 94%;
  margin: 0 3%;
  height: 50rpx;
  display: flex;
  justify-content: space-between;
  font-size: 26rpx;
  align-items: center;
}