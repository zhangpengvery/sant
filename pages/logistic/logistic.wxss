/* pages/logistic/logistic.wxss */
.box-img{
  width: 94%;
  height: 300rpx;
  margin: 20rpx 3% 0 3%;
  box-shadow: 0px 0px 4px rgba(0, 0, 0, 0.14);
  border-radius: 20rpx;
}
.box-img image{
  width: 100%;
  height: 100%;
  border-radius: 20rpx;
}
.box{
  width: 100%;
  height: 650rpx;
}
.box image{
  width: 100%;
  height: 100%;
}
.text{
  color: #686868;
  width: 100%;
  font-size: 20rpx;
  text-align: center;
}