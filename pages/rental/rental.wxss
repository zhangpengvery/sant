.hbtn{
  width: 100%;
  height: 88rpx;
  border-radius: 94rpx;
  background: #F9B236;
  color: #fff;
  text-align: center;
  line-height: 88rpx;
  font-size: 32rpx;
  margin-top: 20rpx;
}
 .list{
  width: 94%;
  height: auto;
  margin: 0 3%;
  display: flex;
  justify-content: space-between;
  flex-wrap: wrap;
  text-align: center;
  overflow: hidden;
}
.list .list-content{
  position: relative;
  width: 44%;
  height: 340rpx;
  padding: 2%;
  border-radius: 20rpx;
  margin-top: 20rpx;
  background: #fff;
}
.list .list-content image{
  width: 284rpx;
  height: 175rpx;
  border-radius: 20rpx ;
  margin-top: 10rpx;
}
.list .list-content .list-name{
  text-align: left;
  padding-left: 3%;
  width: 93%;
  font-size: 28rpx;
  color: #343232;
  height: 40rpx;
  display: block;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  margin: 10rpx 0 5rpx 0;
}
.list .list-content .list-city{
  font-size: 24rpx;
  color: #999;
  width: 93%;
  padding-left: 3%;
  text-align: left;
  margin-bottom: 20rpx;
}
.list .list-content .list-price{
  display: block;
  padding-left: 3%;
  width: 93%;
  text-align: left;
  font-size: 28rpx;
  color: #D90F0F;
}
.list .list-content .list-price ::before{
  content: "￥";
}
.list-right{
  width: 94%;
  height: auto;
  margin: 0 3%;
  overflow: hidden;
}
.list-right .right-content{
  width: 92%;
  padding: 0 3%;
  height: 200rpx;
  border-left: 10rpx solid #F9B236;
  border-radius: 20rpx;
  margin-top: 20rpx;
  background: #fff;
}
.right-content .right-title{
  width: 70%;
  position: relative;
  line-height: 80rpx;
  height: 80rpx;
  color: #333;
  font-size: 32rpx;
  font-weight: bold;
  padding-right: 30%;
  overflow: hidden;
  white-space: nowrap;
  text-overflow:ellipsis;
}
.right-content .right-name{
  color: #999;
  font-size: 24rpx;
  margin-bottom: 10rpx;
}
.right-content .right-title .right-jg{
  position: absolute;
  top: 0;
  right: 0;
  color: #BA1515;
}
.right-content .right-title .right-jg::before{
  content: "￥";
}
.showcity{
  display: flex;
  position: fixed;
  top: 250rpx;
  left: 0;
  width: 94%;
  height: 500rpx;
  padding: 0 3%;
  background: #fff;
  z-index: 60;
}
.showcity .city-content{
  width: 100%;
  height: 100%;
}
.city-content .city-box{
  width: 100%;
  height: 50rpx;
  line-height: 50rpx;
  text-align: center;
  font-size: 26rpx;
}
.city-content .cityac{
  color: #F9B236;
}
.showcity .cityright{
  width: 70%;
  height: auto;
}
.showcity .cityright .right-box{
  float: left;
  height: 50rpx;
  line-height: 50rpx;
  font-size: 26rpx;
  margin: 0 10rpx;
}
